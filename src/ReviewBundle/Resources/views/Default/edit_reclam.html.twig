{%  extends 'admin_base.html.twig' %}

{% block admin_content %}
<div class="content">
    <div class="col-md-8">
    <div class="card">
    <div class="card-header card-header-primary">
        <h4 class="card-title">Edit Complaint</h4>
        <p class="card-category">Edit</p>
    </div>
    <div class="card-body">

        {{ form_start(form)}}



        {{ form_label(form.subject,'Subject',{'attr':{'class':'bmd-label-floating'}}) }}
        {{ form_widget(form.subject,{'attr':{'class':'form-control'}}) }}


        {{ form_label(form.content,'Content',{'attr':{'class':'bmd-label-floating'}}) }}
        {{ form_widget(form.content,{'attr':{'class':'form-control'}}) }}



        {{ form_widget(form.submit) }}
        {{ form_end(form) }}

    </div>
    </div>
{% endblock %}