{% extends 'base.html.twig' %}
{% block content %}

    <footer class="footer-area section-padding-100-6">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header card-header-primary">
                            <h4 class="card-title ">Events list</h4>
                            <p class="card-category"> </p>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead class=" text-primary">
                                    <th>Image</th>
                                    <th>title</th>
                                    <th>start_date</th>
                                    <th>end_date</th>
                                    <th>location</th>
                                    <th>nbr_places</th>
                                    <th>description</th>
                                    <th>type</th>

                                    </thead>
                                    <tbody>
                                    {% for i in t %}
                                        <tr>
                                            <td><img src="{{ asset('assets/img/shop-img/' ~ i.img) }}" height="50px" width="50px"></td>
                                            <td>{{ i.title }}</td>
                                            <td>{{ i.startDate|date("d/m/Y")}}</td>
                                            <td>{{ i.endDate|date("d/m/Y") }}</td>
                                            <td>{{ i.location}}</td>
                                            <td>{{ i.nbPlaces }}</td>
                                            <td>{{ i.description}}</td>
                                            <td>{{ i.type}}</td>



                                        </tr>

                                    {% endfor %}
                                    </tbody>
                                </table>



                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    </div>


{% endblock %}