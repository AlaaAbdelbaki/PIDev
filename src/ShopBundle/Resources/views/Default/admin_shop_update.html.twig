{% extends 'admin_base.html.twig' %}
{% block admin_content %}
    {{ form_start(f) }}
    <table>
        <tr>
            <td>{{ form_label(f.productName,'Product Name') }}</td>
            <td>{{ form_widget(f.productName) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(f.img,'Product Image') }}</td>
            <td>{{ form_widget(f.img) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(f.stock,'Product Stock') }}</td>
            <td>{{ form_widget(f.stock) }}</td>
        </tr>
        <tr>
            <td>{{ form_label(f.price,'Product price') }}</td>
            <td>{{ form_widget(f.price) }}</td>
        </tr>


    </table>

    {{ form_end(f) }}
{% endblock %}